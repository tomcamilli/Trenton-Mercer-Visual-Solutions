<p id="notice"><%= notice %></p>

<h1>Graph Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graph_requests.each do |graph_request| %>
      <tr>
        <td><%= link_to 'Show', graph_request %></td>
        <td><%= link_to 'Edit', edit_graph_request_path(graph_request) %></td>
        <td><%= link_to 'Destroy', graph_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Graph Request', new_graph_request_path %>
